{
  "name": "@fleek-platform/dashboard",
  "version": "0.1.0",
  "scripts": {
    "build": ".scripts/build_static_site",
    "build:ci": "pnpm run build",
    "clean": "rm -rf node_modules src/generated",
    "dev": ".scripts/start_dev_server",
    "dev:check_required": ".scripts/local_requirements",
    "format": "pnpm run format:src && pnpm run format:tests",
    "format:src": "pnpm exec biome format --write ./src",
    "format:check": "pnpm exec biome format ./src ./tests/e2e",
    "format:tests": "pnpm run format:e2e",
    "format:e2e": "pnpm exec biome format --write ./tests/e2e",
    "format:unsafe": "pnpm exec biome check --write --unsafe ./src",
    "generate:sitemap": "pnpm exec bun run .scripts/generate_sitemap.ts",
    "lint": "pnpm exec biome lint --write --changed --no-errors-on-unmatched ./src",
    "lint:check": "pnpm exec biome lint ./src",
    "lint:unsafe": "pnpm exec biome lint --write --unsafe --changed --no-errors-on-unmatched ./src",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build -o .storybook/out",
    "storybook:deploy": "fleek sites deploy -c .storybook/fleek.config.json",
    "test": ".scripts/run_tests",
    "test:component": "pnpm exec vitest run $(find src -name '*.spec.tsx')",
    "test:terminate_http_server": ".scripts/test_terminate_http_server",
    "test:e2e": "pnpm exec playwright test --reporter=list",
    "test:ui": "pnpm exec playwright test --ui",
    "test:unit": "pnpm exec vitest run src/**/*.spec.ts",
    "tsc": "tsc --noEmit",
    "prepare": "husky"
  },
